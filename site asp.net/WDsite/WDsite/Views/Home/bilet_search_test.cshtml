@model WDsite.Models.Bilet_table
@{
    ViewBag.Title = "bilet_search_test";
}
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <h2>bilet_search_test</h2>
    <form action="bilet_search_test" method="post">
        @Html.LabelFor(model => model.form_id, "form_id", htmlAttributes: new { @class = "control-label col-md-2" })
        @Html.DropDownList("form_id", null, htmlAttributes: new { @class = "form-control" })
        @Html.LabelFor(model => model.to_id, "to_id", htmlAttributes: new { @class = "control-label col-md-2" })
        @Html.DropDownList("to_id", null, htmlAttributes: new { @class = "form-control" })
        <button type="submit">
            Send my greetings
        </button>
    </form>
}